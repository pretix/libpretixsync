CREATE TABLE OrderPosition (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    attendee_email TEXT,
    attendee_name TEXT,
    item INTEGER,
    json_data TEXT,
    order_ref INTEGER,
    positionid INTEGER,
    secret TEXT,
    server_id INTEGER,
    subevent_id INTEGER,
    variation_id INTEGER,
    FOREIGN KEY (item) REFERENCES Item (id) ON DELETE CASCADE,
    FOREIGN KEY (order_ref) REFERENCES orders (id) ON DELETE CASCADE
);
