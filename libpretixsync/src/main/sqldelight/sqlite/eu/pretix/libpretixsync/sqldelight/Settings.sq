import kotlin.Boolean;

CREATE TABLE Settings (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    address TEXT,
    city TEXT,
    country TEXT,
    covid_certificates_accept_eudgc INTEGER AS Boolean,
    covid_certificates_accept_manual INTEGER AS Boolean,
    covid_certificates_allow_cured INTEGER AS Boolean,
    covid_certificates_allow_cured_max INTEGER,
    covid_certificates_allow_cured_min INTEGER,
    covid_certificates_allow_other INTEGER AS Boolean,
    covid_certificates_allow_tested_antigen_unknown INTEGER AS Boolean,
    covid_certificates_allow_tested_antigen_unknown_max INTEGER,
    covid_certificates_allow_tested_antigen_unknown_min INTEGER,
    covid_certificates_allow_tested_pcr INTEGER AS Boolean,
    covid_certificates_allow_tested_pcr_max INTEGER,
    covid_certificates_allow_tested_pcr_min INTEGER,
    covid_certificates_allow_vaccinated INTEGER AS Boolean,
    covid_certificates_allow_vaccinated_max INTEGER,
    covid_certificates_allow_vaccinated_min INTEGER,
    covid_certificates_allow_vaccinated_products TEXT,
    covid_certificates_combination_rules TEXT,
    covid_certificates_record_proof INTEGER AS Boolean,
    covid_certificates_record_proof_cured INTEGER AS Boolean,
    covid_certificates_record_proof_other INTEGER AS Boolean,
    covid_certificates_record_proof_tested_antigen_unknown INTEGER AS Boolean,
    covid_certificates_record_proof_tested_pcr INTEGER AS Boolean,
    covid_certificates_record_proof_vaccinated INTEGER AS Boolean,
    covid_certificates_record_validity_time INTEGER AS Boolean,
    json_data TEXT,
    name TEXT,
    pretixpos_additional_receipt_text TEXT,
    slug TEXT,
    tax_id TEXT,
    vat_id TEXT,
    zipcode TEXT
);
