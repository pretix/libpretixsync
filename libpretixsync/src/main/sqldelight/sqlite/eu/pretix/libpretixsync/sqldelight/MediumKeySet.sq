import kotlin.Boolean;

CREATE TABLE MediumKeySet (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    active INTEGER AS Boolean NOT NULL,
    diversification_key TEXT,
    json_data TEXT,
    media_type TEXT,
    organizer TEXT,
    public_id INTEGER,
    uid_key TEXT
);

CREATE INDEX MediumKeySet_public_id_index ON MediumKeySet (public_id);
