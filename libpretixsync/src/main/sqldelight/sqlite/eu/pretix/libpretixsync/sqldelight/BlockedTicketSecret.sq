import kotlin.Boolean;

CREATE TABLE BlockedTicketSecret (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    blocked INTEGER AS Boolean NOT NULL,
    event_slug TEXT,
    json_data TEXT,
    secret TEXT,
    server_id INTEGER,
    updated TEXT
);

CREATE INDEX BlockedTicketSecret_event_slug_index ON BlockedTicketSecret (event_slug);
CREATE INDEX BlockedTicketSecret_secret_index ON BlockedTicketSecret (secret);
