import kotlin.Boolean;

CREATE TABLE ReusableMedium (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    active INTEGER AS Boolean NOT NULL,
    customer_id INTEGER,
    expires TEXT,
    identifier TEXT,
    json_data TEXT,
    linked_giftcard_id INTEGER,
    linked_orderposition_id INTEGER,
    server_id INTEGER,
    type TEXT
);

CREATE INDEX ReusableMedium_identifier_index ON ReusableMedium (identifier);
CREATE INDEX ReusableMedium_server_id_index ON ReusableMedium (server_id);
