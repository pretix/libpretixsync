import kotlin.Boolean;

CREATE TABLE Quota (
    id INTEGER PRIMARY KEY AUTOINCREMENT,
    available INTEGER AS Boolean,
    available_number INTEGER,
    event_slug TEXT,
    json_data TEXT,
    server_id INTEGER,
    size INTEGER,
    subevent_id INTEGER
);

CREATE INDEX Quota_server_id_index ON Quota (server_id);
CREATE INDEX Quota_subevent_id_index ON Quota (subevent_id);
