CREATE TABLE ResuableMedium_OrderPosition (
    ReusableMediumId bigint NOT NULL REFERENCES ReusableMedium(id) ON DELETE CASCADE,
    OrderPositionId bigint NOT NULL REFERENCES OrderPosition(id) ON DELETE CASCADE,
    CONSTRAINT resuablemedium_orderposition_pkey PRIMARY KEY (ReusableMediumId, OrderPositionId)
);
