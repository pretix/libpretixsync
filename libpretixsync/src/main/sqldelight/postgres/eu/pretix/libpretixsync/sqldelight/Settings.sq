import kotlin.Long;

CREATE TABLE Settings (
    id serial AS Long PRIMARY KEY,
    address text,
    city character varying(255),
    country character varying(255),
    covid_certificates_accept_eudgc boolean,
    covid_certificates_accept_manual boolean,
    covid_certificates_allow_cured boolean,
    covid_certificates_allow_cured_max integer,
    covid_certificates_allow_cured_min integer,
    covid_certificates_allow_other boolean,
    covid_certificates_allow_tested_antigen_unknown boolean,
    covid_certificates_allow_tested_antigen_unknown_max integer,
    covid_certificates_allow_tested_antigen_unknown_min integer,
    covid_certificates_allow_tested_pcr boolean,
    covid_certificates_allow_tested_pcr_max integer,
    covid_certificates_allow_tested_pcr_min integer,
    covid_certificates_allow_vaccinated boolean,
    covid_certificates_allow_vaccinated_max integer,
    covid_certificates_allow_vaccinated_min integer,
    covid_certificates_allow_vaccinated_products text,
    covid_certificates_combination_rules text,
    covid_certificates_record_proof boolean,
    covid_certificates_record_proof_cured boolean,
    covid_certificates_record_proof_other boolean,
    covid_certificates_record_proof_tested_antigen_unknown boolean,
    covid_certificates_record_proof_tested_pcr boolean,
    covid_certificates_record_proof_vaccinated boolean,
    covid_certificates_record_validity_time boolean,
    json_data text,
    name character varying(255),
    pretixpos_additional_receipt_text text,
    slug character varying(255),
    tax_id character varying(255),
    vat_id character varying(255),
    zipcode character varying(255)
);
