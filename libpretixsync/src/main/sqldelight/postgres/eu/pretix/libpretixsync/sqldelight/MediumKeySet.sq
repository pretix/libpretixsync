import kotlin.Long;

CREATE TABLE MediumKeySet (
    id serial AS Long PRIMARY KEY NOT NULL,
    active boolean NOT NULL,
    diversification_key character varying(255),
    json_data text,
    media_type character varying(255),
    organizer character varying(255),
    public_id bigint,
    uid_key character varying(255)
);

CREATE INDEX MediumKeySet_public_id_index ON MediumKeySet (public_id);
