import kotlin.Long;

CREATE TABLE ResourceSyncStatus (
    id serial AS Long PRIMARY KEY,
    event_slug character varying(255),
    last_modified character varying(255),
    meta character varying(255),
    resource character varying(255),
    status character varying(255)
);

CREATE INDEX ResourceSyncStatus_event_slug_index ON ResourceSyncStatus (event_slug);
CREATE INDEX ResourceSyncStatus_resource_index ON ResourceSyncStatus (resource);
