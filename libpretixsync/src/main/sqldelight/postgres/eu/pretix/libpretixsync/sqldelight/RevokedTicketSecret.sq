import kotlin.Long;

CREATE TABLE RevokedTicketSecret (
    id serial AS Long PRIMARY KEY NOT NULL,
    created character varying(255),
    event_slug character varying(255),
    json_data text,
    secret character varying(255),
    server_id bigint
);

CREATE INDEX RevokedTicketSecret_event_slug_index ON RevokedTicketSecret (event_slug);
CREATE INDEX RevokedTicketSecret_secret_index ON RevokedTicketSecret (secret);
