import kotlin.Long;

CREATE TABLE OrderPosition (
    id serial AS Long PRIMARY KEY NOT NULL,
    attendee_email character varying(255),
    attendee_name character varying(255),
    json_data text,
    positionid bigint,
    secret character varying(255),
    server_id bigint,
    subevent_id bigint,
    variation_id bigint,
    item bigint REFERENCES Item (id) ON DELETE CASCADE,
    order_ref bigint REFERENCES orders (id) ON DELETE CASCADE
);
